{
    "name": "shipmonk/packing",
    "description": "Testing day application stub",
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=8.4",
        "psr/http-message": "2.*",
        "guzzlehttp/psr7": "2.*",
        "doctrine/orm": "3.*",
        "symfony/cache": "7.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "slevomat/coding-standard": "^8.18"
    },
    "scripts": {
        "check": [
            "@check:cs"
        ],
        "check:cs": "phpcs",
        "fix:cs": "phpcbf"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
